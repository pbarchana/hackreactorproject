nav
  fixed: top
  z-index 100
  background: linear-gradient(top, #3c3c3c, #222);
  width 100%
  height $navHeight

.content
  absolute: top $navHeight left 0px bottom 0px right $sidePanelWidth

.side-panel
  absolute: top $navHeight bottom 0px right 0px
  width: $sidePanelWidth
  padding 10px
  background: url('http://s3.amazonaws.com/linkedin_inmaps_images/images/sidepanel-bg.gif') #dedede repeat-y left